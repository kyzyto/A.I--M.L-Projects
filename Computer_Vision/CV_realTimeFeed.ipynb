   "source": [
    "import cv2\n",
    "\n",
    "cv2.namedWindow(\"SpyCam\")\n",
    "vc = cv2.VideoCapture(0)\n",
    "\n",
    "rval, frame = vc.read()\n",
    "\n",
    "while True:\n",
    "    if frame is not None:\n",
    "        cv2.imshow(\"SpyCam\", frame)\n",
    "    rval, frame = vc.read()\n",
    "    \n",
    "    if cv2.waitKey(1) & 0xFF == ord('q'):\n",
    "        break"
   ]
